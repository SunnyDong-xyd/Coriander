<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout10"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PhysicalFragment">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="367dp"
        android:layout_height="74dp"
        android:layout_marginTop="48dp"
        android:text="Exercise releases endorphins and forces you to regulate your breathing, both of which will help you regain stability"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="367dp"
        android:layout_height="63dp"
        android:layout_marginTop="20dp"
        android:text="Choose from the following exercises:"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <Button
        android:id="@+id/walkbutton"
        android:layout_width="323dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:text="Walk/Jog"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:layout_constraintVertical_bias="0.019" />

    <Button
        android:id="@+id/crunchbutton"
        android:layout_width="323dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:text="Crunches"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/situpbutton"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/situpbutton"
        android:layout_width="323dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:text="Sit-Ups"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jackbutton"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/jackbutton"
        android:layout_width="323dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:text="Jumping Jacks"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/walkbutton"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/pushupbutton"
        android:layout_width="323dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:text="Push-Ups"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/crunchbutton"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>